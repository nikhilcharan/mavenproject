pipeline {
    agent any 
    tools {
        maven 'maven3.6.3'
        jdk 'Java 11.0.8'
    }
    stages {
        stage('Checkout') { 
            steps {
                git credentialsId: 'nikhilcharan', url: 'https://github.com/nikhilcharan/devopstraining.git'
            }
        }  
        
        stage('Build') {
             steps {
                   mvn clean install
           }
        }
    }
  }
